{
    "name": "@pure-function/jsutils",
    "version": "0.2.1",
    "description": "JavaScript utilities library",
    "main": "dist/@pure-function/jsutils.js",
    "unpkg": "dist/@pure-function/jsutils.min.js",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "build": "webpack --env dev && webpack --env build && npm run test",
        "dev": "webpack --progress --colors --watch --env dev",
        "test": "mocha --require babel-register --colors ./test/*.spec.js",
        "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js",
        "test:cover": "cross-env NODE_ENV=test nyc mocha --require babel-register --colors test/*.js",
        "docs:build": "documentation build --config documentation.yml -f html -o docs  src/**"
    },
    "keywords": [
        "javascript",
        "utils",
        "javascript utilities",
        "library"
    ],
    "author": "",
    "license": "Apache",
    "bugs": {
        "url": ""
    },
    "homepage": "https://github.com/pure-function/jsutils",
    "dependencies": {
        "es6-promise": "^4.2.8",
        "lodash.clone": "^4.5.0",
        "lodash.clonedeep": "^4.5.0",
        "lodash.isempty": "^4.4.0",
        "lodash.isequal": "^4.5.0",
        "lodash.uniq": "^4.5.0",
        "omit-deep-lodash": "^1.1.5",
        "selectn": "^1.1.2"
    },
    "devDependencies": {
        "@babel/core": "^7.12.3",
        "@babel/plugin-transform-object-assign": "7.12.1",
        "@babel/preset-env": "^7.12.1",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-add-module-exports": "^1.0.4",
        "babel-plugin-es6-promise": "^1.1.1",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-preset-env": "^7.0.0-beta.3",
        "babel-register": "^7.0.0-beta.3",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "cross-env": "^7.0.2",
        "documentation": "^13.0.2",
        "documentation-theme-light": "^1.1.1",
        "es6-promise": "^4.2.8",
        "eslint": "^7.11.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-jsdoc": "^30.7.3",
        "fs-extra": "^9.0.1",
        "handlebars": "^4.7.6",
        "jsdoc": "^3.6.6",
        "jsdom": "^16.4.0",
        "jsdom-global": "3.0.2",
        "mocha": "^8.2.0",
        "nyc": "^15.1.0",
        "prettier-eslint": "^11.0.0",
        "terser-webpack-plugin": "^5.0.0",
        "webpack": "^5.1.3",
        "webpack-cli": "^4.1.0",
        "yargs": "^16.1.0"
    },
    "nyc": {
        "sourceMap": false,
        "instrument": false
    }
}
